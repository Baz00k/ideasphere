{
  "name": "@ideasphere/nextjs",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "pnpm with-env next build",
    "clean": "git clean -xdf .next .turbo node_modules",
    "dev": "pnpm with-env next dev",
    "lint": "dotenv -v SKIP_ENV_VALIDATION=1 next lint",
    "format": "prettier --check \"**/*.{js,cjs,mjs,ts,tsx,md,json}\"",
    "start": "pnpm with-env next start",
    "typecheck": "tsc --noEmit",
    "with-env": "dotenv -e ../../.env --"
  },
  "dependencies": {
    "@ideasphere/api": "^0.1.0",
    "@ideasphere/db": "^0.1.0",
    "@supabase/auth-helpers-nextjs": "^0.10.0",
    "@supabase/auth-helpers-react": "^0.5.0",
    "@supabase/supabase-js": "^2.42.7",
    "@t3-oss/env-nextjs": "^0.8.0",
    "@tanstack/react-query": "^4.36.1",
    "@tanstack/react-query-devtools": "^4.36.1",
    "@tanstack/react-query-next-experimental": "5.32.0",
    "@trpc/client": "^10.45.2",
    "@trpc/next": "^10.45.2",
    "@trpc/react-query": "^10.45.2",
    "@trpc/server": "^10.45.2",
    "encoding": "^0.1.13",
    "lucide-react": "^0.376.0",
    "next": "^14.2.3",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "superjson": "2.2.2",
    "zod": "^3.23.4"
  },
  "devDependencies": {
    "@ideasphere/eslint-config": "^0.2.0",
    "@ideasphere/prettier-config": "^0.2.0",
    "@ideasphere/tailwind-config": "^0.1.0",
    "@ideasphere/tsconfig": "^0.1.0",
    "@types/node": "^20.12.7",
    "@types/react": "^18.3.1",
    "@types/react-dom": "^18.3.0",
    "dotenv-cli": "^7.4.1",
    "eslint": "^8.57.0",
    "prettier": "^3.2.5",
    "tailwindcss": "3.4.3",
    "typescript": "^5.4.5"
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "@ideasphere/eslint-config/base",
      "@ideasphere/eslint-config/nextjs",
      "@ideasphere/eslint-config/react"
    ]
  },
  "prettier": "@ideasphere/prettier-config"
}
