{
  "name": "@ideasphere/db",
  "version": "0.1.0",
  "private": true,
  "main": "./index.ts",
  "types": "./index.ts",
  "license": "MIT",
  "scripts": {
    "clean": "rm -rf .turbo node_modules",
    "generate": "pnpm with-env prisma generate",
    "push": "pnpm with-env prisma db push --skip-generate",
    "dev": "pnpm with-env prisma studio --port 5556",
    "lint": "eslint .",
    "format": "prettier --check \"**/*.{mjs,ts,md,json}\"",
    "with-env": "dotenv -e ../../.env --"
  },
  "dependencies": {
    "@prisma/client": "^5.7.0"
  },
  "devDependencies": {
    "@ideasphere/eslint-config": "^0.2.0",
    "@ideasphere/prettier-config": "^0.2.0",
    "@ideasphere/tsconfig": "^0.1.0",
    "dotenv-cli": "^7.3.0",
    "eslint": "^8.56.0",
    "prettier": "^3.1.1",
    "prisma": "^5.7.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "prisma": {
    "seed": "ts-node ./prisma/seed.ts"
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "@ideasphere/eslint-config/base"
    ]
  },
  "prettier": "@ideasphere/prettier-config"
}
